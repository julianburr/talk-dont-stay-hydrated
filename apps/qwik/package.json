{
  "private": true,
  "name": "my-qwik-basic-starter",
  "version": "0.0.0",
  "description": "Demo App with Routing built-in (recommended)",
  "cpu": [
    "arm64"
  ],
  "os": [
    "darwin"
  ],
  "engines": {
    "node": ">=15.0.0"
  },
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr --port=4040",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && clear && vite preview --port=4040",
    "start": "vite --mode ssr --port=4040",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.3.5",
    "@builder.io/qwik-city": "^1.3.5",
    "@types/eslint": "^8.56.1",
    "@types/node": "^20.10.8",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-qwik": "^1.3.5",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.1",
    "typescript": "5.3.3",
    "undici": "*",
    "vite": "^5.0.11",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "dependencies": {
    "@workspace/shared": "0.0.0",
    "localstorage-slim": "^2.7.0",
    "markdown-it": "^14.0.0",
    "moment": "^2.30.1",
    "qwik-feather-icons": "^1.2.1",
    "sharp": "^0.33.2",
    "slugify": "^1.6.6"
  }
}
